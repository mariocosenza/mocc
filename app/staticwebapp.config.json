{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/icons/*",
      "/favicon.ico",
      "/manifest.json",
      "/*.js",
      "/*.css",
      "/*.map",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.ico",
      "/*.json",
      "/*.woff",
      "/*.woff2",
      "/*.ttf"
    ]
  },
  "globalHeaders": {
    "Cross-Origin-Opener-Policy": "same-origin-allow-popups",
    "Cross-Origin-Embedder-Policy": "unsafe-none"
  }
}